<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <ImageView
            android:layout_width="0dp"
            android:layout_height="150dp" app:srcCompat="@drawable/devslopesbanner"
            android:id="@+id/appBanner"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:scaleType="centerCrop"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/devslopesprofilelogo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/appLogo"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginStart="24dp"
            android:layout_marginTop="116dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:text="Devslopes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/devslopesText" app:layout_constraintStart_toEndOf="@+id/appLogo"
            app:layout_constraintTop_toBottomOf="@+id/appBanner"
            android:layout_marginStart="24dp" app:layout_constraintHorizontal_bias="0.025"
            android:layout_marginTop="20dp" android:textSize="24sp"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/camerabutton"
            android:id="@+id/imageView9"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/imageView10"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintTop_toTopOf="@+id/imageView10"
            app:layout_constraintBottom_toBottomOf="@+id/imageView10" app:layout_constraintVertical_bias="0.0"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/favoritebutton"
            android:id="@+id/imageView10"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/imageView11"
            app:layout_constraintStart_toEndOf="@+id/imageView9" android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp" app:layout_constraintTop_toTopOf="@+id/imageView11"
            app:layout_constraintBottom_toBottomOf="@+id/imageView11" app:layout_constraintVertical_bias="0.0"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/inboxbutton"
            android:id="@+id/imageView11"
            app:layout_constraintStart_toEndOf="@+id/imageView10" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/appLogo"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline3"
                                          android:orientation="vertical"
                                          app:layout_constraintGuide_percent="0.5"/>
    <ImageView
            android:layout_width="159dp"
            android:layout_height="154dp" app:srcCompat="@drawable/dataimage"
            android:id="@+id/imageView"
            android:scaleType="centerCrop" app:layout_constraintStart_toStartOf="@+id/guideline3"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/imageView4"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintVertical_bias="0.0"/>
    <ImageView
            android:layout_width="154dp"
            android:layout_height="158dp" app:srcCompat="@drawable/imagesimage"
            android:id="@+id/imageView2"
            android:scaleType="centerCrop"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView4"
            app:layout_constraintStart_toStartOf="@+id/imageView4" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintVertical_bias="0.078" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="@+id/imageView4" app:layout_constraintHorizontal_bias="0.111"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/videosimage"
            android:id="@+id/imageView3"
            android:scaleType="centerCrop" app:layout_constraintStart_toStartOf="@+id/guideline3"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView" app:layout_constraintEnd_toEndOf="@+id/imageView"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.062"/>
    <ImageView
            android:layout_width="153dp"
            android:layout_height="154dp" app:srcCompat="@drawable/codeimage"
            android:id="@+id/imageView4"
            android:scaleType="centerCrop" app:layout_constraintEnd_toStartOf="@+id/guideline3"
            android:layout_marginEnd="12dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView9" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintDimensionRatio="h,1:1"
            app:layout_constraintVertical_bias="0.065"/>
</android.support.constraint.ConstraintLayout>